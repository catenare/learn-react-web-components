{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "learn-react-web-components",
  "author": "Johan Martin",
  "module": "dist/learn-react-web-components.esm.js",
  "size-limit": [
    {
      "path": "dist/learn-react-web-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/learn-react-web-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.5.0-alpha.63",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-links": "^6.5.0-alpha.63",
    "@storybook/addons": "^6.5.0-alpha.63",
    "@storybook/react": "^6.5.0-alpha.63",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "babel-loader": "^8.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "^7.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "semantic-release": "^19.0.3",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catenare/learn-react-web-components.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
